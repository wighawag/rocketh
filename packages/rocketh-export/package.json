{
	"name": "rocketh-export",
	"version": "1.0.4",
	"description": "export deployments",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"exports": "./dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"rocketh-export": "dist/cli.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.10.0",
		"abitype": "^0.10.2",
		"rimraf": "^5.0.5",
		"tsup": "^8.0.1",
		"typescript": "^5.3.2"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"chalk": "^5.3.0",
		"commander": "^11.1.0",
		"fs-extra": "^11.1.1",
		"rocketh": "workspace:*"
	},
	"scripts": {
		"build": "rimraf dist && tsup --entry src/index.ts --entry src/cli.ts --dts --format esm,cjs",
		"dev": "rimraf dist && tsup --entry src/index.ts --entry src/cli.ts --dts --format esm,cjs --watch"
	}
}
