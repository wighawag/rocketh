{
	"name": "@rocketh/export",
	"version": "0.10.6",
	"description": "export deployments",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	".": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"bin": {
		"rocketh-export": "dist/cli.cjs"
	},
	"devDependencies": {
		"@types/node": "^20.12.7",
		"abitype": "^1.0.2",
		"eip-1193": "^0.5.0",
		"pkgroll": "^2.0.2",
		"rimraf": "^5.0.5",
		"typescript": "^5.4.5"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"@types/fs-extra": "^11.0.4",
		"chalk": "4.1.2",
		"commander": "^12.0.0",
		"fs-extra": "^11.2.0",
		"rocketh": "workspace:*"
	},
	"scripts": {
		"build": "rimraf dist && pkgroll --sourcemap",
		"dev": "pkgroll --watch"
	}
}
