{
	"name": "rocketh-deploy",
	"version": "3.0.1",
	"description": "provide deploy function for rocketh",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"devDependencies": {
		"abitype": "^1.0.0",
		"eip-1193": "^0.4.7",
		"rimraf": "^5.0.5",
		"rocketh": "workspace:*",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"named-logs": "^0.2.2",
		"viem": "^2.7.9"
	},
	"scripts": {
		"build": "rimraf dist && tsup src/index.ts --dts --format esm,cjs",
		"dev": "rimraf dist && tsup src/index.ts --dts --format esm,cjs --watch",
		"dev-no-reset": "tsup src/index.ts --dts --format esm,cjs --watch"
	}
}
