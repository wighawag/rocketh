{
	"name": "@rocketh/deploy",
	"version": "0.10.8",
	"description": "provide deploy function for rocketh",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	".": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"devDependencies": {
		"abitype": "^1.0.4",
		"eip-1193": "^0.5.0",
		"pkgroll": "^2.1.1",
		"rimraf": "^5.0.7",
		"rocketh": "workspace:*",
		"typescript": "^5.5.2"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"named-logs": "^0.2.4",
		"viem": "^2.16.2"
	},
	"scripts": {
		"build": "rimraf dist && pkgroll --sourcemap",
		"dev": "pkgroll --watch"
	}
}
