{
	"name": "@rocketh/core",
	"version": "0.17.3",
	"description": "utilities for other rocketh packages",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./types": {
			"import": {
				"types": "./dist/types.d.ts",
				"default": "./dist/types.js"
			}
		},
		"./json": {
			"import": {
				"types": "./dist/json.d.ts",
				"default": "./dist/json.js"
			}
		},
		"./artifacts": {
			"import": {
				"types": "./dist/artifacts.d.ts",
				"default": "./dist/artifacts.js"
			}
		},
		"./providers": {
			"import": {
				"types": "./dist/providers/index..d.ts",
				"default": "./dist/providers/index.js"
			}
		},
		"./environment": {
			"import": {
				"types": "./dist/environment.d.ts",
				"default": "./dist/environment.js"
			}
		}
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"abitype": "^1.2.1",
		"as-soon": "^0.0.11",
		"eip-1193": "^0.6.5",
		"rocketh": "workspace:*",
		"typescript": "^5.9.3",
		"viem": "^2.41.2"
	},
	"dependencies": {
		"ethers": "^6.16.0",
		"named-logs": "^0.4.1"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build"
	}
}
