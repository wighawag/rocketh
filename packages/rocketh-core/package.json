{
	"name": "@rocketh/core",
	"version": "0.18.2",
	"description": "utilities for other rocketh packages",
	"keywords": [
		"rocketh",
		"ethereum",
		"deployment",
		"test"
	],
	"author": "Ronan Sandford",
	"license": "MIT",
	"homepage": "https://github.com/wighawag/rocketh/blob/main/packages/rocketh-core/README.md",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wighawag/rocketh.git",
		"directory": "packages/rocketh-core"
	},
	"bugs": {
		"url": "https://github.com/wighawag/rocketh/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./types": {
			"import": {
				"types": "./dist/types.d.ts",
				"default": "./dist/types.js"
			}
		},
		"./json": {
			"import": {
				"types": "./dist/json.d.ts",
				"default": "./dist/json.js"
			}
		},
		"./artifacts": {
			"import": {
				"types": "./dist/artifacts.d.ts",
				"default": "./dist/artifacts.js"
			}
		},
		"./providers": {
			"import": {
				"types": "./dist/providers/index.d.ts",
				"default": "./dist/providers/index.js"
			}
		},
		"./environment": {
			"import": {
				"types": "./dist/environment.d.ts",
				"default": "./dist/environment.js"
			}
		},
		"./account": {
			"import": {
				"types": "./dist/account.d.ts",
				"default": "./dist/account.js"
			}
		}
	},
	"files": [
		"dist",
		"src"
	],
	"dependencies": {
		"abitype": "^1.2.3",
		"eip-1193": "^0.6.5",
		"named-logs": "^0.4.1",
		"viem": "^2.45.1"
	},
	"devDependencies": {
		"as-soon": "^0.1.5",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	},
	"scripts": {
		"prepublishOnly": "cp ../../LICENSE LICENSE",
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build",
		"test": "vitest"
	}
}
