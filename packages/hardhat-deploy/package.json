{
	"name": "hardhat-deploy",
	"version": "2.0.0-alpha.1",
	"description": "deployment plugin for hardhat",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			}
		}
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"@ignored/hardhat-vnext": "3.0.0-next.17",
		"@types/node": "^20.14.8",
		"as-soon": "^0.0.11",
		"rimraf": "^5.0.7",
		"rocketh": "workspace:*",
		"typescript": "^5.5.2"
	},
	"peerDependencies": {
		"@ignored/hardhat-vnext": "3.0.0-next.17",
		"rocketh": "workspace:*"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build"
	}
}
