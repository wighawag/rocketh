{
	"name": "rocketh",
	"version": "0.10.10",
	"description": "deploy smart contract on ethereum-compatible networks",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	".": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"bin": {
		"rocketh": "dist/cli.cjs"
	},
	"devDependencies": {
		"@types/node": "^20.12.7",
		"abitype": "^1.0.2",
		"eip-1193": "^0.5.0",
		"ipfs-gateway-emulator": "4.2.1-ipfs.2",
		"pkgroll": "^2.0.2",
		"rimraf": "^5.0.5",
		"typedoc": "^0.25.13",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@types/figlet": "^1.5.8",
		"@types/prompts": "^2.4.9",
		"commander": "^12.0.0",
		"eip-1193-jsonrpc-provider": "^0.3.0",
		"esbuild": "^0.20.2",
		"esbuild-register": "^3.5.0",
		"ethers": "^6.11.1",
		"figlet": "^1.7.0",
		"ldenv": "^0.3.10",
		"named-logs": "^0.2.4",
		"named-logs-console": "^0.3.1",
		"prompts": "^2.4.2",
		"viem": "^2.9.16"
	},
	"scripts": {
		"build": "rimraf dist && pkgroll --sourcemap",
		"dev": "pkgroll --watch",
		"gen-docs": "typedoc --out docs src",
		"serve-docs": "ipfs-emulator --only -d docs -p 8080"
	}
}
