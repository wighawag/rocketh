{
	"name": "rocketh",
	"version": "0.17.13",
	"description": "core functionality of rocketh",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./types": {
			"import": {
				"types": "./dist/types.d.ts",
				"default": "./dist/types.js"
			}
		}
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@rocketh/core": "workspace:*",
		"abitype": "^1.2.3",
		"change-case": "^5.4.4",
		"eip-1193": "^0.6.5",
		"eip-1193-jsonrpc-provider": "^0.4.3",
		"ldenv": "^0.3.16",
		"named-logs": "^0.4.1",
		"viem": "^2.44.1"
	},
	"devDependencies": {
		"as-soon": "^0.0.11",
		"typescript": "^5.9.3"
	},
	"scripts": {
		"show-chains-with-same-chainId": "tsx show-chains-with-same-chainId.ts",
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build"
	}
}
