			"types": "./dist/esm/index.d.ts",
			"default": "./dist/esm/index.js"
		}
	},
	"bin": {
		"rocketh": "dist/esm/cli.js"
	},
	"devDependencies": {
		"@types/node": "^20.14.8",
		"abitype": "^1.0.4",
		"as-soon": "^0.0.11",
		"eip-1193": "^0.5.0",
		"ipfs-gateway-emulator": "4.2.1-ipfs.2",
		"rimraf": "^5.0.7",
		"typedoc": "^0.26.2",
		"typescript": "^5.5.2"
	},
	"dependencies": {
		"@types/figlet": "^1.5.8",
		"@types/prompts": "^2.4.9",
		"commander": "^12.1.0",
		"eip-1193-jsonrpc-provider": "^0.4.0",
		"ethers": "^6.13.1",
		"figlet": "^1.7.0",
		"ldenv": "^0.3.10",
		"named-logs": "^0.2.4",
		"named-logs-console": "^0.3.1",
		"prompts": "^2.4.2",
		"tsx": "^4.19.2",
		"viem": "^2.16.2"
	},
	"scripts": {
