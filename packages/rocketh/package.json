{
	"name": "rocketh",
	"version": "0.14.8",
	"description": "deploy smart contract on ethereum-compatible networks",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	".": {
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"bin": {
		"rocketh": "dist/cli.js"
	},
	"devDependencies": {
		"@types/node": "^24.4.0",
		"abitype": "^1.1.0",
		"as-soon": "^0.0.11",
		"eip-1193": "^0.6.5",
		"ipfs-gateway-emulator": "4.2.1-ipfs.2",
		"rimraf": "^6.0.1",
		"typedoc": "^0.28.13",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@types/figlet": "^1.7.0",
		"@types/prompts": "^2.4.9",
		"commander": "^13.1.0",
		"eip-1193-jsonrpc-provider": "^0.4.3",
		"ethers": "^6.15.0",
		"figlet": "^1.9.1",
		"ldenv": "^0.3.12",
		"named-logs": "^0.3.2",
		"named-logs-console": "^0.3.1",
		"prompts": "^2.4.2",
		"tsx": "^4.20.5",
		"viem": "^2.37.6"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build",
		"gen-docs": "typedoc --out docs src",
		"serve-docs": "ipfs-emulator --only -d docs -p 8080"
	}
