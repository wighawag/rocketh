{
	"name": "@rocketh/signer",
	"version": "0.10.3",
	"description": "add signer to rocketh",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	".": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"devDependencies": {
		"eip-1193": "^0.5.0",
		"eip-1193-signer": "^0.0.2",
		"pkgroll": "^2.0.2",
		"rimraf": "^5.0.5",
		"rocketh": "workspace:*",
		"typescript": "^5.4.5"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"scripts": {
		"build": "rimraf dist && pkgroll --sourcemap",
		"dev": "pkgroll --watch"
	}
}
