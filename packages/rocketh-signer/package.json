{
	"name": "@rocketh/signer",
	"version": "0.10.7",
	"description": "add signer to rocketh",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	".": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"devDependencies": {
		"eip-1193": "^0.5.0",
		"eip-1193-signer": "^0.0.3",
		"pkgroll": "^2.1.1",
		"rimraf": "^5.0.7",
		"rocketh": "workspace:*",
		"typescript": "^5.5.2"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"scripts": {
		"build": "rimraf dist && pkgroll --sourcemap",
		"dev": "pkgroll --watch"
	}
}
