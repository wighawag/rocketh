{
	"name": "rocketh-deploy-router",
	"version": "1.0.3",
	"description": "provide router based proxy deployment for rocketh",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"devDependencies": {
		"abitype": "^0.10.2",
		"eip-1193": "^0.4.7",
		"rimraf": "^5.0.5",
		"rocketh": "workspace:*",
		"tsup": "^8.0.1",
		"typescript": "^5.3.2"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"ethers": "^6.8.1",
		"named-logs": "^0.2.2",
		"rocketh-deploy": "workspace:*",
		"solidity-proxy": "^0.2.4",
		"viem": "^1.19.8"
	},
	"scripts": {
		"build": "rimraf dist && tsup src/index.ts --dts --format esm,cjs",
		"dev": "rimraf dist && tsup src/index.ts --dts --format esm,cjs --watch"
	}
}
