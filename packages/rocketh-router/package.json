	},
	"dependencies": {
		"@rocketh/deploy": "workspace:*",
		"@rocketh/core": "workspace:*",
		"named-logs": "^0.4.1",
		"solidity-proxy": "^0.4.2",
		"viem": "^2.41.2"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build"
