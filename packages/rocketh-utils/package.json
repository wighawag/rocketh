{
	"name": "@rocketh/utils",
	"version": "0.17.0",
	"description": "utilities for other rocketh packages",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"as-soon": "^0.0.11",
		"rocketh": "workspace:*",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"named-logs": "^0.4.1"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build"
	}
}
