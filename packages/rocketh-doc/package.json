		"typescript": "^5.9.2"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"@types/fs-extra": "^11.0.4",
		"commander": "^13.1.0",
		"ethers": "^6.15.0",
		"fs-extra": "^11.3.1",
		"handlebars": "^4.7.8"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build"
	}
