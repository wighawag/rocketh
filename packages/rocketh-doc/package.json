{
	"name": "@rocketh/doc",
	"version": "0.17.14",
	"description": "can generate doc from rocketh deployments and provided template",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"bin": {
		"rocketh-doc": "dist/cli.js"
	},
	"files": [
		"dist",
		"public",
		"src"
	],
	"dependencies": {
		"@rocketh/core": "workspace:*",
		"@types/fs-extra": "^11.0.4",
		"abitype": "^1.2.3",
		"commander": "^14.0.2",
		"fs-extra": "^11.3.3",
		"handlebars": "^4.7.8",
		"viem": "^2.44.1"
	},
	"devDependencies": {
		"@rocketh/node": "workspace:*",
		"@types/node": "^25.0.6",
		"as-soon": "^0.0.11",
		"rimraf": "^6.1.2",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"@rocketh/node": "workspace:*"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build"
	}
}
