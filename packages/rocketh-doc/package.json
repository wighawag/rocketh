{
	"name": "@rocketh/doc",
	"version": "0.10.5",
	"description": "can generate doc from rocketh deployments and provided template",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	".": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"bin": {
		"rocketh-doc": "dist/cli.cjs"
	},
	"files": [
		"dist",
		"public",
		"src"
	],
	"devDependencies": {
		"@types/node": "^20.12.7",
		"pkgroll": "^2.0.2",
		"rimraf": "^5.0.5",
		"rocketh": "workspace:*",
		"typescript": "^5.4.5"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"@types/fs-extra": "^11.0.4",
		"commander": "^12.0.0",
		"ethers": "^6.11.1",
		"fs-extra": "^11.2.0",
		"handlebars": "^4.7.8"
	},
	"scripts": {
		"build": "rimraf dist && pkgroll --sourcemap",
		"dev": "pkgroll --watch"
	}
}
