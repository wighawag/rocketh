{
	"name": "hardhat-rocketh",
	"version": "4.0.18",
	"description": "rocketh plugin for hardhat",
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			}
		},
		"./helpers": {
			"require": {
				"types": "./dist/helpers.d.ts",
				"default": "./dist/helpers.cjs"
			},
			"import": {
				"types": "./dist/helpers.d.ts",
				"default": "./dist/helpers.mjs"
			}
		}
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"@types/node": "^20.14.8",
		"hardhat": "^2.22.5",
		"pkgroll": "^2.1.1",
		"rimraf": "^5.0.7",
		"rocketh": "workspace:*",
		"typescript": "^5.5.2"
	},
	"peerDependencies": {
		"rocketh": "workspace:*",
		"hardhat": "^2.22.5"
	},
	"scripts": {
		"build": "rimraf dist && pkgroll --sourcemap",
		"dev": "pkgroll --watch"
	}
}
