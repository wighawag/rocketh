{
	"name": "hardhat-rocketh",
	"version": "4.0.0",
	"description": "rocketh plugin for hardhat",
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"devDependencies": {
		"hardhat": "^2.20.1",
		"rimraf": "^5.0.5",
		"rocketh": "workspace:*",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"scripts": {
		"build": "rimraf dist && tsup src/index.ts --dts --format cjs",
		"dev": "rimraf dist && tsup src/index.ts --dts --format cjs --watch"
	}
}
