{
	"name": "hardhat-rocketh",
	"version": "4.0.16",
	"description": "rocketh plugin for hardhat",
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			}
		},
		"./helpers": {
			"require": {
				"types": "./dist/helpers.d.ts",
				"default": "./dist/helpers.cjs"
			},
			"import": {
				"types": "./dist/helpers.d.ts",
				"default": "./dist/helpers.mjs"
			}
		}
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"@types/node": "^20.12.7",
		"hardhat": "^2.22.2",
		"pkgroll": "^2.0.2",
		"rimraf": "^5.0.5",
		"rocketh": "workspace:*",
		"typescript": "^5.4.5"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"scripts": {
		"build": "rimraf dist && pkgroll --sourcemap",
		"dev": "pkgroll --watch"
	}
}
