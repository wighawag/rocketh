{
	"name": "hardhat-rocketh",
	"version": "4.0.9",
	"description": "rocketh plugin for hardhat",
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			}
		},
		"./helpers": {
			"require": {
				"types": "./dist/helpers.d.ts",
				"default": "./dist/helpers.cjs"
			},
			"import": {
				"types": "./dist/helpers.d.ts",
				"default": "./dist/helpers.mjs"
			}
		}
	},
	"devDependencies": {
		"@types/node": "^20.11.19",
		"hardhat": "^2.20.1",
		"pkgroll": "^2.0.2",
		"rimraf": "^5.0.5",
		"rocketh": "workspace:*",
		"typescript": "^5.3.3"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"scripts": {
		"build": "pkgroll --sourcemap",
		"dev": "pkgroll --watch"
	}
}
