{
	"name": "@rocketh/node",
	"version": "0.17.2",
	"description": "rocketh for server-side JS runtime like node, including a cli",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"bin": {
		"rocketh": "dist/cli.js"
	},
	"devDependencies": {
		"@rocketh/core": "workspace:*",
		"@types/node": "^24.10.2",
		"as-soon": "^0.0.11",
		"ipfs-gateway-emulator": "4.2.1-ipfs.2",
		"rimraf": "^6.1.2",
		"rocketh": "workspace:*",
		"typedoc": "^0.28.15",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"@types/prompts": "^2.4.9",
		"change-case": "^5.4.4",
		"commander": "^14.0.2",
		"eip-1193": "^0.6.5",
		"ldenv": "^0.3.16",
		"prompts": "^2.4.2",
		"tsx": "^4.21.0",
		"viem": "^2.41.2"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build",
		"gen-docs": "typedoc --out docs src",
		"serve-docs": "ipfs-emulator --only -d docs -p 8080"
	}
}
