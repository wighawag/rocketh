{
	"name": "@rocketh/diamond",
	"version": "0.17.13",
	"description": "provide diamond support for rocketh/hardhat-deploy",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"default": "./dist/types.js"
		},
		"./solc_0_8/*": "./solc_0_8/*"
	},
	"files": [
		"dist",
		"src",
		"solc_0_8"
	],
	"dependencies": {
		"@rocketh/core": "workspace:*",
		"@rocketh/deploy": "workspace:*",
		"@rocketh/read-execute": "workspace:*",
		"abitype": "^1.2.3",
		"eip-1193": "^0.6.5",
		"named-logs": "^0.4.1",
		"viem": "^2.45.0"
	},
	"devDependencies": {
		"@rocketh/test-utils": "workspace:*",
		"as-soon": "^0.1.4",
		"rimraf": "^6.1.2",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	},
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "as-soon -w src pnpm build",
		"test": "vitest"
	}
}
