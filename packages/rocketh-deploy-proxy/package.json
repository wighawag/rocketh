{
	"name": "@rocketh/deploy-proxy",
	"version": "0.10.4",
	"description": "provide proxy-enabled deploy function for rocketh",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	".": {
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"devDependencies": {
		"abitype": "^1.0.2",
		"eip-1193": "^0.5.0",
		"pkgroll": "^2.0.2",
		"rimraf": "^5.0.5",
		"rocketh": "workspace:*",
		"typescript": "^5.4.5"
	},
	"peerDependencies": {
		"rocketh": "workspace:*"
	},
	"dependencies": {
		"named-logs": "^0.2.4",
		"@rocketh/deploy": "workspace:*",
		"solidity-proxy": "^0.4.0",
		"viem": "^2.9.16"
	},
	"scripts": {
		"build": "rimraf dist && pkgroll --sourcemap",
		"dev": "pkgroll --watch"
	}
}
